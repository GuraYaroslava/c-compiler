int main()
{
    int i = 0;
    for (i = 0; i < 5; i += 1)
    {
        int j = -1;
        while (j < i * i)
        {
            j += 1;
            if (j < i)
                continue;
            if (j >= i + 1)
                break;
            printf("%d %d\n", j, i);
        }
    }
    return 0;
}